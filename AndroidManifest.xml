<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.example"
      android:versionCode="6"
      android:versionName="6.0">
    <supports-screens android:anyDensity="true"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <application android:label="@string/app_name">
        <uses-library android:name="com.google.android.maps"/>
        <activity android:name="MyActivity" android:label="@string/app_name"/>
        <activity android:name=".MojMikroAndroidActivity" android:label="Moj Mikro Android"
                  android:icon="@drawable/launcher_icon">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".MessagesListActivity"/>
        <activity android:name=".MessagesMapActivity"/>
        <activity android:name=".SendMessageActivity"/>
    </application>
</manifest> 
